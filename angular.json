    "website": {
      "projectType": "application",
      "root": "apps/website",
      "sourceRoot": "apps/website/src",
      "prefix": "lla",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/website",
            "index": "apps/website/src/index.html",
            "main": "apps/website/src/main.ts",
            "polyfills": "apps/website/src/polyfills.ts",
            "tsConfig": "apps/website/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/website/src/favicon.ico",
              "apps/website/src/assets/",
              {
                "glob": "**/*",
                "input": "libs/shared/ui-library/src/lib/styles/common/assets",
                "output": "/common/assets"
              },
              {
                "glob": "**/*",
                "input": "libs/shared/ui-library/src/lib/styles/vtr/assets",
                "output": "/vtr/assets"
              },
              {
                "glob": "**/*",
                "input": "libs/shared/ui-library/src/lib/styles/vtr-b2b/assets",
                "output": "/vtr-b2b/assets"
              },
              {
                "glob": "**/*",
                "input": "libs/shared/ui-library/src/lib/styles/lcpr/assets",
                "output": "/lcpr/assets"
              },
              {
                "glob": "**/*",
                "input": "libs/shared/ui-library/src/lib/styles/flow/assets",
                "output": "/flow/assets"
              },
              {
                "glob": "*.*",
                "input": "libs/shared/ui-library/src/lib/styles/vtr/assets/fonts",
                "output": "vtr"
              },
              {
                "glob": "*.*",
                "input": "libs/shared/ui-library/src/lib/styles/vtr-b2b/assets/fonts",
                "output": "vtr-b2b"
              },
              {
                "glob": "*.*",
                "input": "libs/shared/ui-library/src/lib/styles/lcpr/assets/fonts",
                "output": "lcpr"
              },
              {
                "glob": "*.*",
                "input": "libs/shared/ui-library/src/lib/styles/flow/assets/fonts",
                "output": "flow"
              }
            ],
            "styles": [
              {
                "input": "libs/shared/ui-library/src/lib/styles/vtr/index.scss",
                "bundleName": "vtr/index",
                "inject": false
              },
              {
                "input": "libs/shared/ui-library/src/lib/styles/vtr-b2b/index.scss",
                "bundleName": "vtr-b2b/index",
                "inject": false
              },
              {
                "input": "libs/shared/ui-library/src/lib/styles/lcpr/index.scss",
                "bundleName": "lcpr/index",
                "inject": false
              },
              {
                "input": "libs/shared/ui-library/src/lib/styles/flow/index.scss",
                "bundleName": "flow/index",
                "inject": false
              }
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/website/src/environments/environment.ts",
                  "with": "apps/website/src/environments/environment.prod.ts"
                },
                {
                  "replace": "apps/website/src/app/build-specifics/index.ts",
                  "with": "apps/website/src/app/build-specifics/index.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "30kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "website:build",
            "proxyConfig": "apps/website/proxy.conf.json"
          },
          "configurations": {
            "production": {
              "browserTarget": "website:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "website:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/website/src/**/*.ts",
              "apps/website/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/website"],
          "options": {
            "jestConfig": "apps/website/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
